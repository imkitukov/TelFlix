@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>_ProfileLayout</title>
</head>
<body>
    <div class="container">
        <div class="row">
            <div class="col-md-3">
                <div class="nav flex-column nav-pills" id="v-pills-tab" role="tablist" aria-orientation="vertical">
                    <a class="nav-link active" id="v-pills-account-tab" data-toggle="pill" href="" role="tab" aria-controls="v-pills-account" aria-selected="true">Account</a>
                    <a class="nav-link" id="v-pills-library-tab" data-toggle="pill" href="" role="tab" aria-controls="v-pills-library" aria-selected="false">Library</a>
                    <a class="nav-link" id="v-pills-wishlist-tab" data-toggle="pill" href="" role="tab" aria-controls="v-pills-wishlist" aria-selected="false">Wishlist</a>
                    <a class="nav-link" id="v-pills-reviews-tab" data-toggle="pill" href="" role="tab" aria-controls="v-pills-reviews" aria-selected="false">Reviews</a>
                </div>
            </div>
            <div class="col-md-6">
                @RenderBody()
            </div>
        </div>
    </div>
</body>
</html>
